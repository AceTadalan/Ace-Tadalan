#include <iostream>
using namespace std;

int main () {
	
	char c;
	cout << "Enter a character";
	cin >> c;
	
	switch (c) {
		
		case 'a':
		case 'A':
		cout << "This is a vowel";
		break;
		
		case 'e':
		case 'E':
		cout << "This is a vowel";
		break;
		
		case 'i':
		case 'I':
		cout << "This is a vowel";
		break;
		
		case 'o':
		case 'O':
		cout << "This is a vowel";
		break;
		
		case 'u':
		case 'U':
		cout << "This is a vowel";
		break;
		
		default:
			cout << "This is a consonant";
			break;
	}

	return 0;
}
